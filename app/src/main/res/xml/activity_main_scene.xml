<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnClick motion:targetId="@+id/imageView6"
            motion:clickAction="toggle" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_marginEnd="16dp"
            android:layout_height="75dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="85dp"
            android:id="@+id/imageView5"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="204dp"
            motion:layout_constraintStart_toStartOf="@+id/imageView"
            android:layout_marginStart="310dp"
            android:layout_marginLeft="310dp" />
        <Constraint
            android:layout_height="75dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="85dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"
            android:id="@+id/imageView3"
            android:layout_marginBottom="204dp"
            android:layout_marginLeft="16dp"
            motion:layout_constraintEnd_toEndOf="@+id/imageView"
            android:layout_marginEnd="310dp"
            android:layout_marginRight="310dp" />
        <Constraint
            android:layout_width="219dp"
            android:layout_height="215dp"
            motion:layout_editor_absoluteY="80dp"
            motion:layout_editor_absoluteX="96dp"
            android:id="@+id/imageView2"
            motion:layout_constraintTop_toTopOf="@+id/imageView"
            android:layout_marginTop="80dp"
            motion:layout_constraintEnd_toEndOf="@+id/imageView"
            android:layout_marginEnd="96dp"
            android:layout_marginRight="96dp"
            motion:layout_constraintStart_toStartOf="@+id/imageView"
            android:layout_marginStart="96dp"
            android:layout_marginLeft="96dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView"
            android:layout_marginBottom="436dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_marginEnd="76dp"
            android:layout_height="75dp"
            android:layout_marginRight="76dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="85dp"
            android:id="@+id/imageView5"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="132dp"
            android:visibility="invisible" />
        <Constraint
            android:layout_height="75dp"
            motion:layout_constraintStart_toStartOf="@+id/imageView3"
            android:layout_marginBottom="132dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView"
            motion:layout_constraintHorizontal_bias="0.356"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="85dp"
            android:id="@+id/imageView4"
            android:visibility="invisible" />
        <Constraint
            android:layout_height="75dp"
            android:layout_width="85dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="76dp"
            android:layout_marginStart="76dp"
            android:id="@+id/imageView3"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="132dp"
            android:visibility="invisible" />
    </ConstraintSet>
</MotionScene>